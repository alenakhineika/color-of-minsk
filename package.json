{
  "name": "color-of-minsk",
  "displayName": "color-of-minsk",
  "version": "0.0.1",
  "description": "The color of the sky in Minsk",
  "scripts": {
    "start": "tsc && node build/index.js",
    "lint": "eslint '**/*.{js,ts}'",
    "check": "npm run lint",
    "prettier-format": "run-script-os",
    "prettier-format:win32": "prettier --config .prettierrc \"./src/**/*.ts\" --write",
    "prettier-format:darwin:linux": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-format:default": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "dependencies": {
    "canvas": "2.6.1",
    "chalk": "^4.1.0",
    "color-convert": "^2.0.1",
    "dotenv": "8.2.0",
    "sharp": "^0.27.2",
    "twitter": "1.7.1",
    "typescript": "^4.2.2"
  },
  "devDependencies": {
    "@types/twitter": "^1.7.0",
    "prettier": "2.2.1"
  }
}
